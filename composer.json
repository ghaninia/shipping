{
    "name": "ghaninia/shipping" ,
    "type": "package",
    "description": "iranian shipping",
    "authors": [
        {
            "name": "Amin Ghaninia",
            "email": "info@ghaninia.ir",
            "homepage": "https://ghaninia.ir"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.4",
        "hekmatinasser/verta": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "GhaniniaIR\\Shipping\\" : "src/"
        } ,
        "files": [
            "src/index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GhaniniaIR\\Shipping\\ShippingServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
